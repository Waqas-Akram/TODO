<%-include("partials/header")-%>

    <div class="box" id="heading">
    <h1> <%= kindOfDay %></h1>
</div>
<div class="box">
       <% pushedHere.forEach(function(insert){%>
        <form action="/deleteItems" method="POST">
        <div class="item">
            <input type="checkbox" name ="deleteCheck" value="<%=insert._id %>" onchange="this.form.submit()">
            <p><%= insert.item_name%></p>
        </div>
    </form>
      <%  })%>  
    <form  class="item" action="/" method="POST">
    <input type="text" name="menuName" placeholder="Add new item" autofocus="on" autocomplete="off">
    <button type="submit" name="button">+</button>
</form>
<%-include("partials/footer")-%>