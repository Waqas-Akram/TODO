<%-include('partials/header')-%>
<div class="container-fluid jumbotron">
    <center>
<form action="/contact" method="POST">
<div class="form-group">
  <label for="yourName">Your Name</label>  
<input type="text" class="form-control name" name="yourName" placeholder="enter here your name">
</div>
<div class="form-group">
    <label for="yourEmail"> Your Email</label>
    <input  type="email" name="yourEmail" class="form-control name" placeholder="enter your email">

</div>
<div class="form-group">
    <label for="yourMessage">Your Message</label>
<textarea class="form-control name" name="yourMessage" rows="10" cols="12"></textarea>
</div>
<button type="submit"  class="btn btn-lg btn-primary myButton"> Send</button>
</form>

</div>
</center>

<%-include('partials/footer')-%>